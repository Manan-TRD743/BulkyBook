<div class="card shadow borderborder-0 mt-4">
    <div class="card-header bg-secondary bg-gradient ml-0 py-3">
        <div class="col-12 text-center">
            <h2 class="text-white py-2">Transaction Histroy</h2>
            <hr />
        </div>
    </div>
</div>

<div class="card-body p-4 mb-4">
    <div class="d-flex justify-content-between pb-1 pt-2">
        <span></span>
    </div>
    <div class="card-body mb-4">
        <form id="searchForm" class="form-inline">
            <div class="form-group mr-2">
                <label class="text-date" for="start-date">Start Date:</label>
                <input type="date" id="start-date" name="start-date" class="form-control">
            </div>

            <div class="form-group mr-2">
                <label class="text-dark" for="end-date">End Date:</label>
                <input type="date" id="end-date" name="end-date" class="form-control">
            </div>

            <button type="button" id="searchButton" class="btn btn-primary mt-2">Search</button>
        </form>
    </div>

    <table id="tblData" class="table table-bordered table-striped ">
        <thead>
            <tr>
                <th>Name</th>
                <th>Order Status</th>
                <th>Payment Status</th>
                <th>Amount</th>
                <th>Payment Date</th>
            </tr>
        </thead>
    </table>

</div>

@section Scripts {
    <script src="~/js/GetTransactionReport.js"></script>

}

